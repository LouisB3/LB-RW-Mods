<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RecipeDef ParentName="SurgeryFlesh">
    <defName>RebuildJaw</defName>
    <label>rebuild jaw</label>
    <description>Plastic surgery to reconstruct a jaw that is shattered, destroyed, or otherwise missing. This reverses the associated difficulties with eating and talking. Plasteel is used as a replacement for damaged or absent bone and enamel. Due to the nature of the surgery, industrial medicine or better is required.</description>
    <workerClass>Recipe_InstallArtificialBodyPart</workerClass>
    <jobString>Rebuilding jaw.</jobString>
    <workAmount>2000</workAmount>
    <skillRequirements>
      <Medicine>6</Medicine>
    </skillRequirements>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Medicine</li>
          </categories>
        </filter>
        <count>1</count>
      </li>
      <li>
         <filter>
           <thingDefs>
             <li>Plasteel</li>
           </thingDefs>
         </filter>
         <count>10</count>
       </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>Medicine</li>
      </categories>
	  <disallowedThingDefs>
        <li>MedicineHerbal</li>
      </disallowedThingDefs>
	  <thingDefs>
        <li>Plasteel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Jaw</li>
    </appliedOnFixedBodyParts>
  </RecipeDef>

